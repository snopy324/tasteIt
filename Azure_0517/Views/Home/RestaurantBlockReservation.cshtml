@model IGrouping<int, Azure_0517.Models.ReservationOpened>

@if (Model != null)
{
    foreach (var opened in Model)
    {
        <div class="newBookli nav-item col-3">
            <div class="slide-btn  hover">
                <a href="#" class="LVM newbook" data-hover="訂位" data-id="@opened.RVOpen_ID" data-seatremain="@opened.RVOpen_SeatsRemain">
                    @opened.RVOpen_DateTime.ToString("HH:mm")
                    <small>@opened.RVOpen_OffPrice</small>
                </a>
            </div>
        </div>
    }
}